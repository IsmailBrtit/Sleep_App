<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>

            <corners android:radius="30dp"/>

            <gradient
                android:startColor="#5587F8"
                android:centerColor="#1E32B5"
                android:endColor="#021235"
                android:angle="45" />
            <stroke android:color="#283593"
                android:width="1dp"/>

        </shape>
        <ripple android:color="@color/teal_200"/>
        <transition android:opacity="translucent"/>



    </item>
</selector>